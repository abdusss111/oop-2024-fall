package problem2;

public class Test {

	public static void main(String[] args) {
		Position pos = new Position('d', 5);
		Position pos2 = new Position('c', 7);
		Knight kn = new Knight(pos, Color.BLACK);
		
		System.out.println(kn);
		System.out.println(kn.isLegalMove(pos2));
		
		Position pos3 = new Position('d', 5);
		Position pos4 = new Position('c', 7);
		Bishop bsh = new Bishop(pos3, Color.WHITE);

		System.out.println(bsh);
		System.out.println(bsh.isLegalMove(pos4));

	}

}

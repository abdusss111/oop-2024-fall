package problem2;

public class Test {

	public static void main(String[] args) {
		Position pos = new Position('h', 7);
		Position pos2 = new Position('h', 5);
		Pawn p1 = new Pawn(pos, Color.BLACK);
		Pawn p2 = new Pawn(pos, Color.WHITE);
		
		System.out.println(p1.isLegalMove(pos2));
		System.out.println(p1.rowDifference(pos2));
		
		System.out.println(p2.isLegalMove(pos2));
		System.out.println(p2.rowDifference(pos2));

		

	}

}

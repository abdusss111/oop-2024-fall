package problem2;

public class Board {
	public String drawBoard() {
		String result = "";
		String columns = "abcdefgh";
		for(int i = 0; i < 8; i++) {
			result += columns.charAt(i);
			for(int j = 0; j < 9; j++) {
				result += " |q| ";
			}
			result += "\n" ;
			result += "\n" ;

		}
		return result;
	}
	
	public Board() {}
}
